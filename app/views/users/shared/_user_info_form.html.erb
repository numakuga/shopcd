<div class="field">
  <%= f.label :お名前 %>
  <%= f.label :（姓） %>
  <%= f.text_field :first_name, autofocus: true, autocomplete: "first_name" %>
  <%= f.label :（名） %>
  <%= f.text_field :last_name, autocomplete: "last_name" %>
</div>

<div class="field">
  <%= f.label :お名前（カナ） %>
  <%= f.label :（セイ） %>
  <%= f.text_field :first_kana, autocomplete: "first_kana" %>
  <%= f.label :（メイ） %>
  <%= f.text_field :last_kana, autocomplete: "last_kana" %>
</div>

<div class="field">
  <%= f.label :郵便番号 %>
  <%= f.text_field :postal_code, autocomplete: "postal_code" %>
</div>

<div class="field">
  <%= f.label :住所 %>
  <%= f.text_field :address, autocomplete: "address" %>
</div>

<div class="field">
  <%= f.label :電話番号 %>
  <%= f.text_field :phone, autocomplete: "phone" %>
</div>

<div class="field">
  <%= f.label :メールアドレス %>
  <%= f.email_field :email, autocomplete: "email" %>
</div>
